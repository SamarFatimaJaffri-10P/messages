{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "npm-start",
            "type": "npm",
            "script": "start:dev",
            "runOptions": {"runOn": "folderOpen"}
        },
        {
            "label": "nest-module",
            "type":"shell",
            "command": "nest generate module module_name",
            "runOptions": {"runOn": "default"}
        },
        {
            "label": "nest-controller",
            "type": "shell",
            "command": "nest generate controller  controller_path/name --flat",
            "runOptions": {"runOn": "default"}
        },
        
    ]
}